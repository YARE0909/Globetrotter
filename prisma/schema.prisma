generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  userName    String @unique
  password    String
  displayName String
  score       Int    @default(0)
}
